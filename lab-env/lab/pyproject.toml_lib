[build-system]
requires = ["setuptools>=61.0"]
build-backend = "setuptools.build_meta"

[project]
name = "hello"
version = "0.0.1"
authors = [
  { name="TeamCdR2024" },
]
description = "Example of a Python module calling C code"
readme = "README.md"
requires-python = ">=3.7"
classifiers = [
    "Programming Language :: Python :: 3",
    "License :: OSI Approved :: MIT License",
    "Operating System :: OS Independent",
]

[project.urls]
"Homepage" = "https://github.com/hdumcke/multipass-orchestrator-configurations.git"
